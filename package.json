{
  "name": "lakitu",
  "version": "0.1.0",
  "private": true,
  "description": "Self-hosted Convex agent running in E2B sandbox with browser automation",
  "type": "module",
  "scripts": {
    "dev": "convex dev",
    "deploy": "convex deploy",
    "generate": "convex codegen",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:convex": "convex run tests/runner:runAll",
    "test:integration": "bun run tests/integration/run.ts",
    "typecheck": "tsc --noEmit",
    "template:build": "bun template/build.ts",
    "template:push": "bun template/build.ts --push",
    "template:base": "bun template/build.ts --base --push",
    "template:custom": "bun template/build.ts --custom --push"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "@ai-sdk/openai": "^3.0.10",
    "@openrouter/ai-sdk-provider": "^1.5.4",
    "@convex-dev/agent": "^0.2.0",
    "ai": "^6.0.34",
    "convex": "^1.17.0",
    "e2b": "^2.10.1",
    "loro-crdt": "^1.0.0",
    "openapi-fetch": "^0.10.0",
    "pdfkit": "^0.15.0",
    "playwright": "^1.48.0",
    "zod": "^3.23.0",
    "zod-to-json-schema": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/pdfkit": "^0.13.0",
    "convex-test": "^0.0.34",
    "typescript": "^5.6.0",
    "vitest": "^2.0.0"
  }
}
